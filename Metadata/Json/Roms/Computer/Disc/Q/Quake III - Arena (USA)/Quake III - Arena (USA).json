            },
            {
                "from": "$SETUP_MAIN_ROOT/fix/release-mingw64-x86_64.zip",
                "to": "$HDD_MAIN_ROOT/Quake3",
                "type": "extract"
            },
            {
                "from": "$SETUP_MAIN_ROOT/fix/Point-Release-1.32-PAK.zip",
                "to": "$HDD_MAIN_ROOT/Quake3/baseq3",
                "type": "extract"
            }
        ]
    },
    "transform_file": "Quake III - Arena (USA).chd"
}