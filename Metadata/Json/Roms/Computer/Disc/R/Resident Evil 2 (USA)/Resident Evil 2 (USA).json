{
    "main_game_exe": [
        "ResidentEvil2/bio2 1.10.exe"
    ],
    "main_game_exe_cwd": {
        "ResidentEvil2/bio2 1.10.exe": "ResidentEvil2"
    },
    "sandbox": {
        "wine": {
            "overrides": {
                "game": [
                    "ddraw=n,b"
                ]
            }
        }
    },
    "steps": {
        "preinstall": [
            {
                "from": "$DISC_THREE_ROOT/data",
                "to": "$HDD_MAIN_ROOT/ResidentEvil2",
                "type": "copy"
            },
            {
                "from": "$DISC_FOUR_ROOT/*",
                "to": "$HDD_MAIN_ROOT/ResidentEvil2",
                "type": "copy"
            },
            {
                "from": "$SETUP_MAIN_ROOT/fix/*",
                "to": "$HDD_MAIN_ROOT/ResidentEvil2",
                "type": "copy"
            }
        ]
    },
    "transform_file": "Resident Evil 2 (USA).m3u"
}